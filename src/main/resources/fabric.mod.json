{
  "schemaVersion": 1,
  "id": "catsexpanded",
  "version": "$version",

  "name": "Cats Expanded",
  "description": "Adds more cat stuff to Minecraft!",
  "authors": [
    "xipit"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/Xipit/cats-expanded"
  },

  "license": "MIT",
  "icon": "assets/catsexpanded/icon.png",

